{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Yarp": "Information"
    }
  },
  "ReverseProxy": {
    "Routes": {
      "api": {
        "ClusterId": "api1",
        "Match": {
          "Path" : "api/{*slug}"
        }
      }
    },
    "Clusters": {
      "api1": {
        "Destinations": {
          "api1/destination1": {
            "Address":  "http://10.0.0.199:3102"
          }
        }
      }
    }
  }
}
